<section id="viewLogs" class="bigView">
  <div class="logs">
    {{>pagination}}
    <div class="table-responsive">
    <table class="table table-striped table-hover  table-condensed">
      <thead class="thead-dark">
        <tr>
          <th>User</th>
          <th>Activity</th>
          <th>Time of occurance</th>
        </tr>
      </thead>
      <tbody>
        {{#each logs}}
          <tr>
            <td>{{user}}</td>
            <td>{{activity}}</td>
            <td>{{timeOfOccurrence}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
  </div>
</section>
